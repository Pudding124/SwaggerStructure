      "maximum": 48000,
      "minimum": 8000,
      "type": "integer"
    },
    "NextToken": {
      "maxLength": 8192,
      "type": "string"
    },
    "NotFoundException": {},
    "OutputBucketName": {
      "pattern": "[a-z0-9][\\.\\-a-z0-9]{1,61}[a-z0-9]",
      "type": "string"
    },
    "OutputLocationType": {
